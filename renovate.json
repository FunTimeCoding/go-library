{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "k8s.io/api",
        "k8s.io/client-go",
        "k8s.io/metrics",
        "k8s.io/apimachinery"
      ],
      "allowedVersions": "<= v0.30.14"
    },
    {
      "matchPackageNames": ["github.com/google/gnostic-models"],
      "allowedVersions": "<= 0.6.9"
    }
  ]
}
